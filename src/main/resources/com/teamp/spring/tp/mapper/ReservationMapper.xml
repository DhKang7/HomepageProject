<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.teamp.spring.tp.mapper.ReservationMapper">
	
	<select id="list" resultType="com.teamp.spring.tp.dto.ReservationDto">
		select * from tb_example where ex_list = #{ex_list}
	</select>
	
	<delete id = "del">
		delete from tb_example where ex_book_number = #{ex_book_number}
	</delete>
	
	<insert id="add">
	    INSERT INTO tb_example (ex_book_number, ex_book_date) VALUES (#{ex_book_number}, #{ex_book_date})
	</insert>

	<update id="modify">
		update tb_example
		set ex_book_date = #{ex_book_date}
		where ex_book_number = #{ex_book_number}
	</update>
</mapper>